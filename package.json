    "karma-coverage": "^1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-jsdom-launcher": "~7.1.0",
    "karma-webpack": "~1.8.1",
    "lodash": "~4.17.20",
    "ng-annotate-webpack-plugin": "0.1.3",
    "numeral": "^2.0.6",
