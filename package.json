    "es7-shim": "^6.0.0",
    "sprintf-js": "^1.1.1"
  },
  "resolutions": {
    "angular": "1.8.3",
    "angular-animate": "1.8.3",
    "angular-sanitize": "1.8.3",
    "get-intrinsic": "1.3.0"
  }
